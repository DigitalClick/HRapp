<?xml version="1.0" encoding="utf-8"?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="io.cordova.MobApp1" version="1.0.0" xmlns="http://www.w3.org/ns/widgets">
  <name></name>
  <description>Company Suite</description>
  <author href="http://cordova.io" email="dev@cordova.apache.org">Digital Team </author>
  <content src="Login.html" />
  <icon src="/images/icons/AppIcon.png"/>
  <access origin="*" launch-external="yes" />
  <access origin="tel:*" launch-external="yes" />
  <access origin="mailto:*" launch-external="yes" />
  <access origin="sms:*" launch-external="yes" />
  <access origin="sip:*" launch-external="yes"/>
  <access origin="skype:*" launch-external="yes" />
  <preference name="SplashScreen" value="screen" />
  <preference name="windows-target-version" value="8.0" />
  <preference name="windows-phone-target-version" value="8.1" />
  <preference name="Fullscreen" value="false" />
  <preference name="ShowTitle" value="False" />
  <preference name="Orientation" value="portrait" />
  <vs:plugin name="com.phonegap.plugins.speech" version="1.0.0" />
  <vs:plugin name="org.apache.cordova.plugin.tts" version="0.2.1" />
  <vs:plugin name="io.cordova.plugins.TTS" version="0.3.0" />
  <vs:platformSpecificValues />
</widget>