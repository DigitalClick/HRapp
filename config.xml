<?xml version="1.0" encoding="utf-8"?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="com.phonegap.GoDigital" version="2.0.0" xmlns="http://www.w3.org/ns/widgets">
  <name>GoDigital</name>
  <description>GoDigital</description>
  <author href="http://GoDigital.com" email="support@GoDigital.com">
    GoDigital Team
  </author>
  <content src="Login.html" />
  <icon src="/images/icons/AppIcon.png" />
  <access origin="*" />
  <access origin="tel:*" launch-external="yes" />
  <access origin="mailto:*" launch-external="yes" />
  <access origin="sms:*" launch-external="yes" />
  <access origin="im:*" launch-external="yes" />
  <access origin="sip:*" launch-external="yes" />
  <access origin="skype:*" />


  
  <feature name="Capture">
        <param name="ios-package" value="CDVCapture" />
    <plugin name="Media" value="CDVSound" />
  </feature>
  <plugin name="Media" value="org.apache.cordova.AudioHandler" />
  
  <preference name="SplashScreen" value="screen" />
  <preference name="windows-target-version" value="8.0" />
  <preference name="windows-phone-target-version" value="8.1" />
  <preference name="Fullscreen" value="false" />
  <preference name="ShowTitle" value="False" />
  <preference name="Orientation" value="portrait" />
  <preference name="apiKey" value="4b4d30a41960749591a32924f6ec2e41" />
  <preference name="permissions" value="none" />
  <preference name="orientation" value="default" />
  <preference name="target-device" value="universal" />
  <preference name="fullscreen" value="true" />
  <preference name="webviewbounce" value="true" />
  <preference name="prerendered-icon" value="true" />
  <preference name="stay-in-webview" value="false" />
  <preference name="ios-statusbarstyle" value="black-opaque" />
  <preference name="detect-data-types" value="true" />
  <preference name="exit-on-suspend" value="false" />
  <preference name="show-splash-screen-spinner" value="true" />
  <preference name="auto-hide-splash-screen" value="true" />
  <preference name="disable-cursor" value="false" />
  <preference name="android-installLocation" value="auto" />
  <vs:plugin name="org.apache.cordova.media" version="0.2.13" />
  <vs:plugin name="com.lampa.startapp" version="0.0.4" />
  <vs:platformSpecificValues />


  <platform name="android">
    <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
  </platform>
  
</widget>